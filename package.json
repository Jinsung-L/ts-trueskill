{
  "name": "ts-trueskill",
  "version": "1.0.0",
  "description": "typescript trueskill port",
  "main": "build/index.js",
  "repository": "https://github.com/scttcper/ts-trueskill.git",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "mocha --require ts-node/register test/*.spec.ts",
    "coverage": "nyc npm test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.33",
    "mocha": "^3.2.0",
    "nyc": "^10.0.0",
    "ts-node": "^1.7.0",
    "tslint": "^4.0.2",
    "typescript": "^2.1"
  },
  "nyc": {
    "sourceMap": true,
    "instrument": true,
    "require": [
      "ts-node/register"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": []
  }
}
