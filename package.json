{
  "name": "ts-trueskill",
  "version": "0.0.0-placeholder",
  "description": "Port of python trueskill package in TypeScript",
  "source": "src/public_api.ts",
  "main": "./ts-trueskill.js",
  "module": "./ts-trueskill.m.js",
  "typings": "./public_api.d.ts",
  "sideEffects": false,
  "repository": "scttcper/ts-trueskill",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "scripts": {
    "benchmark": "ts-node -P ./benchmark/tsconfig.json benchmark/index.ts",
    "lint": "tslint -p tsconfig.json -t stylish 'src/**/*.ts' 'test/**/*.ts'",
    "lint:fix": "tslint -p tsconfig.json -t stylish --fix 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "microbundle build --compress false --format cjs,es --target node -o dist",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src && touch docs/.nojekyll",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:prod": "npm run test -- --coverage --no-cache",
    "commit": "git-cz",
    "travis-deploy-once": "travis-deploy-once",
    "presemantic-release": "cp package.json dist && cp LICENSE dist && cp README.md dist",
    "semantic-release": "cd dist && semantic-release"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "mathjs": "^5.1.2",
    "ts-gaussian": "^1.1.9",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/benchmark": "1.0.31",
    "@types/fs-extra": "5.0.4",
    "@types/jest": "23.3.2",
    "@types/lodash": "4.14.116",
    "@types/mathjs": "4.4.1",
    "@types/node": "10.11.0",
    "@types/uuid": "3.4.4",
    "benchmark": "2.1.4",
    "commitizen": "2.10.1",
    "cz-conventional-changelog": "2.1.0",
    "jest": "23.6.0",
    "microbundle": "0.6.0",
    "rimraf": "2.6.2",
    "semantic-release": "15.9.16",
    "travis-deploy-once": "5.0.8",
    "ts-jest": "23.10.1",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-config-standard": "8.0.1",
    "typedoc": "0.12.0",
    "typescript": "3.0.3"
  },
  "release": {
    "branch": "master"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "testURL": "http://localhost",
    "transform": {
      ".ts": "ts-jest"
    },
    "testRegex": "(/test/.*|\\.(spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true
  },
  "keywords": [
    "typescript",
    "trueskill",
    "player skill"
  ]
}
