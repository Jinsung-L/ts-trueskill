{
  "compileOnSave": false,
  "compilerOptions": {
    "moduleResolution": "node",
    "lib": ["es2017"],
    "target": "es2015",
    "module": "es2015",
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "sourceMap": true
  },
	"typedocOptions": {
    "out": "docs",
    "entryPoint": "TrueSkill",
		"mode": "file",
		"target": "ES6",
		"module": "commonjs",
		"moduleResolution": "node",
		"exclude": "source/test",
		"ignoreCompilerErrors": true,
		"excludePrivate": true,
		"excludeNotExported": true,
		"stripInternal": true,
		"suppressExcessPropertyErrors": true,
		"suppressImplicitAnyIndexErrors": true,
		"theme": "minimal",
		"hideGenerator": true,
		"readme": "none"
  }
}
